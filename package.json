  "license": "MIT",
  "dependencies": {
    "@graphile-contrib/pg-simplify-inflector": "^4.0.0-alpha.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "postgraphile": "^4.3.3"
  },
  "scripts": {
    "start": "node index.js",
